:hex4
	ANDI32_RAX %0xF                             ; isolate nibble
	ADDI8_to_RAX !48                            ; convert to ascii
	CMP_AL_Immediate8 !57                       ; valid digit?
	JBE8 !hex1                                  ; yes
	ADDI8_to_RAX !7                             ; use alpha range
:hex1
	STORE8_AL_into_Address_RCX                  ; store result
	ADDI8_to_RCX !1                             ; next position
	RET


