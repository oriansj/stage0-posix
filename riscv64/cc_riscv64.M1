    RD_A3 RS1_A1 MV                   ; Put BOOL in place
    RD_A4 MV                          ; PASSED = 0

    RD_A0 ~function_call_string_0 AUIPC ; Using "ERROR in process_expression_list\nNo ( was found\n"
    RD_A0 RS1_A0 !function_call_string_0 ADDI
    RD_A1 ~open_paren AUIPC           ; Using "("
