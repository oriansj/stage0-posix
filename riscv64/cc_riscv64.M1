    RD_A0 ~function_call_string_7 AUIPC ; Using "RD_A0 RS1_FP !-"
    RD_A0 RS1_A0 !function_call_string_7 ADDI
    RD_RA $emit_out JAL               ; Emit it

    RD_A0 RS1_A2 MV                   ; Using S
