	LOAD64_RBX_from_ABS32 &function             ; Using function
	LOAD64_into_RBX_from_Address_RBX_Immediate8 !8  ; i = function->locals
	LOADI32_RAX &recursive_statement_string_1   ; Using "POP_ebx\t# _recursive_statement_locals\n"

